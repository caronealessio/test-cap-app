<mvc:View controllerName="appcap.appcap.controller.Home"
  xmlns:mvc="sap.ui.core.mvc"
  xmlns="sap.m"
  xmlns:u="sap.ui.unified">
  <Page id="page"
    title="My App">
    <content>
      <Button id="button"
        text="Converti in Intero"
        press=".onCallToInteger" />
      <u:FileUploader id="fileUploader"
        name="file"
        change="onFileChange"
        fileType="pdf" />

      <Button text="Upload Single Fattura"
        press="onUploadPress"
        enabled="true" />
    </content>
  </Page>

</mvc:View>